{
    "manifest_version": 3,
    "name": "test",
    "version": "1.0",
    "description": "test",
    "icons": {
        "16": "assets/icon16.png",
        "48": "assets/icon48.png",
        "128": "assets/icon128.png"
    },
    "action": {
        "default_title": "test",
        "default_popup": "popup.html"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "assets/*",
                "*.js",
                "*.png",
                "*.css",
                "*.json",
                "*.html"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "content_scripts": [
        {
            "matches": [
                "https://*.instagram.com/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_idle"
        }
    ],
    "permissions": [
        "tabs"
    ],
    "host_permissions": [
        "https://*.instagram.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    }
}